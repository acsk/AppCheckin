{
  "name": "frontendweb",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node -r ./node-polyfill.js ./node_modules/.bin/expo start",
    "android": "node -r ./node-polyfill.js ./node_modules/.bin/expo start --android",
    "ios": "node -r ./node-polyfill.js ./node_modules/.bin/expo start --ios",
    "web": "node -r ./node-polyfill.js ./node_modules/.bin/expo start --web --port 8082",
    "web:dev": "node -r ./node-polyfill.js bash -c 'source .env.development && ./node_modules/.bin/expo start --web --port 8082'",
    "web:prod": "node -r ./node-polyfill.js bash -c 'source .env.production && ./node_modules/.bin/expo start --web --port 8082'",
    "web:build:dev": "bash -c 'source .env.development && expo export --platform web --clear && cp -r assets dist/'",
    "web:build": "bash -c 'source .env.production && expo export --platform web --clear && cp -r assets dist/'",
    "copy-icons": "node scripts/copy-icons.js",
    "postbuild": "node scripts/copy-icons.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/stack": "^7.6.13",
    "axios": "^1.13.2",
    "expo": "~54.0.30",
    "expo-linear-gradient": "~15.0.8",
    "expo-router": "^6.0.21",
    "expo-status-bar": "~3.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-modal": "^3.16.3",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-web": "^0.21.0",
    "react-select": "^5.10.2",
    "tailwindcss": "^3.4.19"
  },
  "private": true,
  "devDependencies": {
    "typescript": "~5.9.2"
  }
}
